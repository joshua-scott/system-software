#include <stdio.h>
#include <stdlib.h>

int main (void)
{
    // int n = 0;
    // scanf("%d", &n);
    // printf("testing\n");
    // printf("%d\n", n);

    int numbers[10];
    for(int i = 0; i < 10; i++)
    {
    	fflush(stdout);
        scanf("%d", &numbers[i]);
        printf("\nYou entered %d\n", numbers[i]);
    }
    for(int i = 0; i < 10; i++)
    {
        printf("\nNumber %d was %d", i, numbers[i]);
    }
}
